/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  width:100%;
  margin:20px auto;
}
.banner{
  width:100%;
  height: 100%;
}
.swiper-itemV{
  width:100%;
  height: 100%;
}

.swiper-itemV image{
  width:100%;
  height: 175%;
}
.clView{
  width:100%;
  font-size: 40rpx;
  line-height: 250%;
  margin: 10px 0;
  color: #666;
  text-indent: 10px;
  border-top:1px solid #eee;
  background: #fff;
  border-bottom:1px solid #eee;
  position: relative;
}
.dircText{
  color: #999;
  position: absolute;
  top:8px;
  right:10px;
  width:64rpx;
  height: 64rpx;
  display: block;
}
.dircText image{
  width:64rpx;
  height: 64rpx;
  display: block;
  flex: 1;
}
.borWidth{
  width:100%;
  border-bottom: 1px #eee solid;
}
.scrollW{
  width:100%;
  white-space:nowrap;
}
.modelV{
  width:300rpx;
  height: 300rpx;
  text-align: center;
  border-radius: 3px;
  background: #666;
  display: inline-block;
  margin: 0 5px;
  margin-bottom: 5px;
}

.movie1{
  background: #fdab43;
}
.movie2{
  background: #61cb85;
}
.movie3{
  background: #ad78e1;
}
.movie4{
  background: #fa85a5;
}
.modelImg{
  width:90%;
  height: 55px;
  margin: 5px auto;
  margin-top:40rpx;
}
.modelImg image{
  width:100%;
  height: 100%;
}
.modelF{
  width:100%;
  font-size: 28rpx;
  line-height: 20px;
  color: #fff;
  font-weight: bold;
  margin-top: 10px;
}
.models{
  width: 100%;
  font-size: 22rpx;
  line-height: 16px;
  color: #fff;
}